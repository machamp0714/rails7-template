{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.1",
    "@tanstack/react-query": "^4.1.3",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "esbuild": "^0.14.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-router-dom": "6"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
    "build:js": "esbuild app/javascript/packs/*.* --bundle --sourcemap --outdir=app/assets/builds",
    "failure:js": "rm ./app/assets/builds/application.js && rm ./app/assets/builds/application.js.map",
    "dev:js": "tsc-watch --noClear -p tsconfig.json --onSuccess \"yarn build:js\" --onFailure \"yarn failure:js\"",
    "browser:sync": "browser-sync start --proxy \"localhost:3000\" --files \"app/assets/builds/*.js\" --port 3001",
    "storybook": "start-storybook -p 6006 -s app/javascript/src/public",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "2",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/babel__core": "^7.1.19",
    "@types/browser-sync": "^3001.0.28-alpha",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/tsc-watch": "^4.2.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "chromatic": "^6.7.0",
    "msw": "^0.44.2",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4",
    "typesync": "^0.9.2"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
